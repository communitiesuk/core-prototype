{% extends "layouts/question.njk" %}

{% set title = "Which scheme does this log relate to?" %}
{% set questionId = "schemeId" %}
{% set required = true %}

{% block question %}
  {{ govukRadios(decorate({
    fieldset: {
      legend: {
        html: macro.heading(title, caption)
      }
    },
    items: data.questions.scheme | optionItems("name", "id", "hint")
  }, ["logs", log.id, section.id, questionId])) }}
{% endblock %}
